{
	"name": "@ctx-core/flash",
	"version": "9.0.23",
	"description": "ctx-core flash",
	"keywords": [
		"ctx-core",
		"FileReader",
		"file-reader"
	],
	"homepage": "https://github.com/ctx-core/flash#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/flash/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/flash.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "^11.1.16",
		"@ctx-core/function": "^20.8.0",
		"@ctx-core/nanostores": "^1.0.17",
		"@ctx-core/object": "^22.1.10",
		"svelte": "^3.48.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.177",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}
