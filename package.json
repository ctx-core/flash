{
	"name": "@ctx-core/flash",
	"version": "5.0.18",
	"description": "ctx-core flash",
	"keywords": [
		"ctx-core",
		"FileReader",
		"file-reader"
	],
	"homepage": "https://github.com/ctx-core/flash#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/flash/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/flash/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^10.0.15",
		"@ctx-core/function": "workspace:^19.1.10",
		"@ctx-core/object": "workspace:^20.2.2",
		"@ctx-core/store": "workspace:^26.0.17",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.14",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
