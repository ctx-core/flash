{
	"name": "@ctx-core/flash",
	"version": "11.0.53",
	"description": "ctx-core flash",
	"keywords": [
		"ctx-core",
		"FileReader",
		"file-reader"
	],
	"homepage": "https://github.com/ctx-core/flash#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/flash/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/flash.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"flash",
		"flash__expire__timeout_id",
		"flash_error",
		"flash_message",
		"flash_queue",
		"flash_timeout"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/dom": "^15.3.12",
		"ctx-core": "^5.33.0"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}